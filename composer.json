{
    "name": "postyou/contao-facebook-connector_basic-bundle",
    "description": "Facebook Connector for Contao - Basic Version.",
    "keywords": [
        "contao",
        "facebook",
        "connector",
        "symfony",
        "bundle",
        "events",
        "posts",
        "galleries"
    ],
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Mario Gienapp",
            "role": "Developer",
            "homepage": "http://dyingmountain.de"
        },
        {
            "name":"postyou",
            "homepage":"http://postyou.de"
        }
    ],
    "support": {
        "email": "kontakt@postyou.de"
    },
    "require": {
        "php": ">=5.6.0",
        "contao/core-bundle": "~4.3",
        "facebook/graph-sdk": "~5.0"
    },
    "autoload" : {
      "psr-4": {
          "Postyou\\ContaoFacebookConnectorBasicBundle\\" : "src/"
      },
      "classmap": [
        "src/Resources/contao"
      ],
      "exclude-from-classmap" : [
          "src/Resources/contao/config/",
          "src/Resources/contao/dca/",
          "src/Resources/contao/languages/",
          "src/Resources/contao/templates/"
      ]
    },
    "extra": {
        "contao-manager-plugin": "Postyou\\ContaoFacebookConnectorBasicBundle\\ContaoManager\\Plugin"
    }
}
